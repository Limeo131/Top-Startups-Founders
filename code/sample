o_drop = []\n","    for i in range(len(sample_df) - 1):\n","        if sample_df.iloc[i].equals(sample_df.iloc[i + 1]):\n","            # Mark the index of the row to be dropped\n","            index_to_drop.append(i + 1)\n","\n","    # Drop the marked rows\n","    sample_df = sample_df.drop(index_to_drop)\n","\n","    sample_df = sample_df.reset_index(drop=True)\n","\n","    return sample_df\n"],"metadata":{"id":"EfdORSlvPoOu","executionInfo":{"status":"ok","timestamp":1713821030241,"user_tz":300,"elapsed":4,"user":{"displayName":"limeolivemylife","userId":"07661152881313167081"}}},"execution_count":4,"outputs":[]},{"cell_type":"code","source":["grants['project_end_date'].values[0][0:4]"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":35},"id":"UizkxkASyMQh","executionInfo":{"status":"ok","timestamp":1713821030242,"user_tz":300,"elapsed":4,"user":{"displayName":"limeolivemylife","userId":"07661152881313167081"}},"outputId":"2d97d8d9-9442-40e5-a458-c7893af2799f"},"execution_count":5,"outputs":[{"output_type":"execute_result","data":{"text/plain":["'2004'"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"string"}},"metadata":{},"execution_count":5}]},{"cell_type":"code","source":["people_sample    = []\n","grants_sample    = []\n","papers_sample    = []\n","companies_sample = []\n","patents_sample   = []\n","\n","people_sample = sample(people)\n","grants_sample = sample(grants)\n","papers_sample = sample(papers)\n","papers_sample_new = sample(papers_new)\n","#companies_sample = sample(companies)\n","patents_sample = sample(patents)"],"metadata":{"id":"Mtm3ErCVPoWW","executionInfo":{"status":"ok","timestamp":1713821053414,"user_tz":300,"elapsed":23175,"user":{"displayName":"limeolivemylife","userId":"07661152881313167081"}}},"execution_count":6,"outputs":[]},{"cell_type":"code","source":["for i in range(len(companies)):\n","    if pd.isna(companies['Most Recent Valuation (MM)'].loc[i]):\n","        companies.loc[companies['Most Recent